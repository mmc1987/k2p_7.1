
config wifi-device 'ra0'
	option type 'mt_dbdc'
	option hwmode '11g'
	option channel 'auto'
	option htmode 'HT20'
	option country 'CN'
	option txburst '1'
	option smart '1'

config wifi-iface 'default_ra0'
	option device 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'CMCC'
	option encryption 'psk-mixed'
	option key '1234567890'

config wifi-device 'rax0'
	option type 'mt_dbdc'
	option hwmode '11a'
	option channel 'auto'
	option htmode 'VHT80'
	option country 'CN'
	option txburst '1'
	option noscan '1'
	option smart '0'

config wifi-iface 'default_rax0'
	option device 'rax0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ASUS_5G'
	option encryption 'psk-mixed'
	option key 'XLGG1987'

